# (SP pc=1) /sp/* -> /*  (2015/7/7 fork)
RewriteCond	%{ENV:UA}		^sp$
RewriteCond	%{QUERY_STRING}	(^|&)pc=1($|&)
RewriteCond	%{REQUEST_URI}	^/sp(/|/.*/|/.*\.html|/.*\.php\d?)$
RewriteCond	%{DOCUMENT_ROOT}%1 -f [OR]
RewriteCond	%{DOCUMENT_ROOT}%1index.html -f
RewriteRule	^.*$			%1	[NE,R=302,L]

RewriteCond	%{ENV:UA}		^sp$
RewriteCond	%{QUERY_STRING}	(^|&)pc=1($|&)
RewriteCond	%{REQUEST_URI}	^/sp(/$|/.*/$|/.*\.html$|/.*\.php\d?)$
RewriteRule	^.*$			/	[R=302,L]


